{
  "version": 2,
  "functions": {
    "api/submit.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/submit.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/submit",
      "dest": "/api/submit.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}